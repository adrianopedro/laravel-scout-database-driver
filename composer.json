{
    "name": "adrianopedro/laravel-scout-database-driver",
    "description": "Driver for Laravel Scout database search package",
    "keywords": ["search", "search", "scout", "laravel"],
    "license": "MIT",
    "authors": [{
        "name": "Adriano Pedro",
        "email": "adrianopedro@gmail.com"
    }],
    "require": {
        "php": ">=7.2",
        "illuminate/bus": ">=8.0",
        "illuminate/contracts": ">=8.0",
        "illuminate/database": ">=8.0",
        "illuminate/pagination": ">=8.0",
        "illuminate/queue": ">=8.0",
        "illuminate/support": ">=8.0",
        "laravel/scout": ">=9.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "AdrianoPedro\\Scout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        },
        "laravel": {
            "providers": [
                "AdrianoPedro\\Scout\\APSearchScoutServiceProvider"
            ]
        }
    },
    "suggest": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
